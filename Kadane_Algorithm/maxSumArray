   def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if len(nums) == 1:
            return nums[0]
        current = nums[0]
        max_sum = nums[0]
        
        for i in nums[1:]:
            current = max(i, current + i)
            max_sum = max(max_sum, current)
        return max_sum
