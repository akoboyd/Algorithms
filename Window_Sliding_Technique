class Solution(object):
    def numSubarrayProductLessThanK(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: int
        """
        if k < 0:
            return 0
        start = end = result = 0
        p = 1
        for i in range(len(nums)):
            p *= nums[i]
            while p >= k and start < end:
                p /= nums[start]
                start += 1
            if p < k:
                result += end - start + 1
            end += 1
        return result
